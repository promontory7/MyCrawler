<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'  
        'http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd'>
<hibernate-mapping package="cn.wa8.qweb.extract.bean">
	<class name="model.Project" table="project">
		<!-- 映射标识属性 -->
		<id name="id" type="string">
			<!-- 指定主键生成器策略 -->
			<generator class="uuid" />
		</id>
		<property name="websiteType"></property>
		<property name="projectName"></property>
		<property name="projectNo"></property>
		<property name="projectBrief" length="4000"></property>
		<property name="projectAddress"></property>
		<property name="projectType"></property>
		<property name="projectStage"></property>
		<property name="projectPrice"></property>
		<property name="tenDerWay"></property>
		<property name="planCycle"></property>
		<property name="publicStart"></property>
		<property name="publicEnd"></property>
		<property name="finalEnd"></property>
		<property name="owners"></property>
		<property name="ownerpeopleName"></property>
		<property name="ownerpeoplePhone"></property>
		<property name="agency"></property>
		<property name="agencyName"></property>
		<property name="agecyPhone"></property>
		<property name="bond"></property>
		<property name="bonder"></property>
		<property name="isAcceptUnion"></property>
		<property name="qualification_requir"></property>
		<property name="manager_require"></property>
		<property name="article" length="4000"></property>
		<property name="attach" length="4000"></property>
		<property name="state"></property>
	</class>
</hibernate-mapping>  


